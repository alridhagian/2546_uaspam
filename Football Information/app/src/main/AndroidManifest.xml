<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.asy.footballinformation">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label= "@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.asy.footballinformation.activity.SearchTeamActivity"></activity>
        <activity android:name="com.asy.footballinformation.activity.DetailTeamActivity" />
        <activity android:name="com.asy.footballinformation.activity.DetailMatchActivity" />
        <activity android:name="com.asy.footballinformation.activity.SearchResultActivity" />
        <activity android:name="com.asy.footballinformation.activity.DetailLigaActivity" />
        <activity android:name="com.asy.footballinformation.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>